{
  "name": "xml-ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"json-server --watch src/assets/mock/database.json\" \"ng serve\"",
    "ng": "ng",
    "https": "ng serve --ssl=true --sslCert=ssl/server.crt --sslKey=ssl/server.key",
    "local": "ng serve --host=0.0.0.0 --ssl=true --sslCert=ssl/server.crt --sslKey=ssl/server.key",
    "debug:ios": "remotedebug_ios_webkit_adapter --port=4200",
    "start": "ng serve",
    "start-sandbox": "ng serve -c sandbox",
    "build": "ng build",
    "build-sandbox": "ng build -c sandbox --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer --output-hashing=all",
    "build-staging": "ng build -c staging --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer --output-hashing=all",
    "build-prod": "ng build -c production --aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer --output-hashing=all",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^5.1.1",
    "@angular-material-components/moment-adapter": "^5.0.0",
    "@angular-slider/ngx-slider": "^2.0.4",
    "@angular/animations": "^12.2.17",
    "@angular/cdk": "^12.2.13",
    "@angular/cli": "^12.2.18",
    "@angular/common": "^12.2.17",
    "@angular/compiler": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/forms": "^12.2.17",
    "@angular/material": "^12.2.13",
    "@angular/material-moment-adapter": "^12.2.13",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "^12.2.17",
    "@angular/platform-server": "^12.2.17",
    "@angular/router": "^12.2.17",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nicky-lenaers/ngx-scroll-to": "^9.0.0",
    "@types/googlemaps": "^3.37.7",
    "angular-calendar": "^0.28.28",
    "angular-email-editor": "^0.9.0",
    "angular-svg-icon": "^10.0.0",
    "angularx-qrcode": "^11.0.0",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^1.0.0",
    "chartjs-plugin-labels": "^1.1.0",
    "ckeditor4-angular": "^3.1.0",
