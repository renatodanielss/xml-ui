    "ngx-infinite-scroll": "^10.0.1",
    "ngx-mask": "^11.1.5",
    "ngx-slick-carousel": "^0.5.1",
    "npm": "^6.14.17",
    "object-hash": "^2.2.0",
    "rxjs": "~6.6.7",
    "sass": "^1.52.1",
